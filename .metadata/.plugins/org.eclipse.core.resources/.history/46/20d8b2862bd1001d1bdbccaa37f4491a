<mapper namespace="com.projectmoon.persistence.CdCommonMapper">
	
	<!-- 코드 불러오기 -->
	<select id="selectCode" parameterType="com.projectmoon.cdCommon.CdCommonVO" resultType="com.projectmoon.cdCommon.CdCommonVO">
		SELECT 
			CODE
			, MAIN_CATEGORY
			, PROPERTIES  
		FROM 
			CD_CODE 
		WHERE 
			CODE = #{code}
	</select>
	
</mapper>
